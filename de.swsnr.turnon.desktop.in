[Desktop Entry]
Name=Turn On
Comment=Turn on devices in your network
Exec=de.swsnr.turnon
Type=Application
Icon=de.swsnr.turnon
Categories=GNOME;GTK;Utility;
Keywords=Gnome;GTK;WakeOnLan;
StartupNotify=true
DBusActivatable=true
Actions=add-device;

[Desktop Action add-device]
Name=Add new device
Exec=de.swsnr.turnon --add-device
